{
    "hierarchy": {
        "READ": 1,
        "WRITE": 2,
        "ADMIN": 3
    },
    "bypass": {
        "adminRoles": [
            "ADMIN"
        ],
        "superAdminFlag": "isAdmin",
        "inactiveDeny": true
    },
    "defaults": {
        "userReadAll": {
            "enabled": true,
            "level": "READ",
            "resourceTypes": [
                "PRODUCT",
                "SOLUTION",
                "CUSTOMER"
            ]
        }
    },
    "resourceInheritance": [
        {
            "source": "SOLUTION",
            "target": "PRODUCT",
            "direction": "BOTH",
            "logic": "If user has access to a solution, they have access to its products. If user has access to all products in a solution, they have access to that solution (lowest permission level applies)."
        }
    ],
    "systemRoles": [
        "ADMIN",
        "SME",
        "CSS",
        "USER",
        "VIEWER"
    ]
}